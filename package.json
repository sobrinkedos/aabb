{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "VITE_ENVIRONMENT=production vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:auth": "vitest run src/contexts/__tests__ src/utils/__tests__ src/__tests__/integration/auth-flow.test.tsx",
    "test:auth:watch": "vitest src/contexts/__tests__ src/utils/__tests__ src/__tests__/integration/auth-flow.test.tsx",
    "test:coverage": "vitest run --coverage",
    "perf:enable": "node scripts/performance-test.js enable",
    "perf:disable": "node scripts/performance-test.js disable",
    "perf:status": "node scripts/performance-test.js status",
    "perf:test": "node scripts/performance-test.js",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "node scripts/deploy.js --prod",
    "deploy:check": "node scripts/deploy.js --check-only",
    "commit:optimizations": "node scripts/commit-optimizations.js",
    "release": "npm run commit:optimizations && npm run deploy:prod",
    "backup:db": "tsx scripts/backup-database.ts",
    "restore:db": "tsx scripts/restore-database.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.56.1",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
